{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client1",
      "image": "pavlovelykyi/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker1",
      "image": "pavlovelykyi/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server1",
      "image": "pavlovelykyi/multi-sever",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx1",
      "image": "pavlovelykyi/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "server1",
        "client1"
      ],
      "memory": 128
    }
  ]
}